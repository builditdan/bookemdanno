


    <% if like = current_user.liked(bookmark) %>
    <%= link_to [topic, bookmark, like], class: 'btn-xs btn-danger', method: :delete do %>
      <i class="glyphicon glyphicon-star-empty"> </i>&nbsp; Unlike &nbsp;
    <% end %>
  <% else %>
    <%= link_to [topic, bookmark, Like.new], class: 'btn-xs btn-primary bookmark-list', method: :post do %>
      <i class="glyphicon glyphicon-star"> </i>&nbsp; Like
    <% end %>
  <% end %>
